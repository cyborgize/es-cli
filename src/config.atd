
type config = {
  ~clusters : (string * cluster_config) list <json repr="object">;
  ?version : version option;
}

type cluster_config = {
  ?host : string option;
  ?nodes : string list option;
  ?version : version option;
}

type version = int wrap <ocaml
  t="[ `ES5 | `ES6 | `ES7 ]"
  wrap="function 5 -> `ES5 | 6 -> `ES6 | 7 -> `ES7 | x -> Devkit.Exn.fail \"unknown ES version %d\" x"
  unwrap="function `ES5 -> 5 | `ES6 -> 6 | `ES7 -> 7"
>
