
type json <ocaml module="Yojson.Safe" t="t"> = abstract

type config = {
  ~clusters : (string * cluster_config) list <json repr="object">;
  ?version : version option;
}

type cluster_config = {
  ?host : string option;
  ?nodes : string list option;
  ?version : version option;
}

type version = json wrap <ocaml module="Wrap.Version">
